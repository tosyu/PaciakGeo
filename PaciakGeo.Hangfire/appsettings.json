{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "logs/log.txt" } }
    ]
  },
  "NodeBBConfig": {
    "ServiceUrl": "https://paciak.pl",
    "UserAgent": "PaciakGeo"
  },
  "UpsertPaciakUsersJob": "*/15 * * * *",
  "UpdateUsersStaticLocationJob": "* * * * *",
  "AllowedHosts": "*"
}
